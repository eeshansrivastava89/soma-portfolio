{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"strictNullChecks": true,
		"allowJs": true,
		"baseUrl": ".",
		"lib": ["es2022", "dom", "dom.iterable"],
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "**/node_modules/*", ".vscode", "dist"]
}
